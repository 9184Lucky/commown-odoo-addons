<odoo>

  <record id="view_website_form" model="ir.ui.view">
    <field name="name">website.form (in the website_sale_b2b module)</field>
    <field name="model">website</field>
    <field name="inherit_id" ref="website.view_website_form"/>
    <field name="arch" type="xml">

      <xpath expr="//group[@name='other']/group" position="inside">
        <field name="login_checkbox_message"/>
        <field name="legal_notice_url"/>
      </xpath>

    </field>
  </record>


</odoo>
