<odoo>

  <record id="action_wizard_project_task_print_label" model="ir.actions.act_window">
    <field name="name">[commown] SUPPORT,GESTION: print label</field>
    <field name="src_model">project.task</field>
    <field name="res_model">commown_shipping.task.print_label.wizard</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
    <field name="context">{'default_shipping_ids': active_ids}</field>
    <field name="binding_model_id" ref="project.model_project_task" />
  </record>

  <record id="action_wizard_crm_lead_print_label" model="ir.actions.act_window">
    <field name="name">[commown] Print label</field>
    <field name="src_model">crm.lead</field>
    <field name="res_model">commown_shipping.lead.print_label.wizard</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
    <field name="context">{'default_shipping_ids': active_ids}</field>
    <field name="binding_model_id" ref="crm.model_crm_lead" />
  </record>

</odoo>
